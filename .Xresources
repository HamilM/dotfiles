URxvt.font: xft:DejaVu Sans mono for Powerline:size=14
URxvt.scrollBar:false
urxvt*depth: 32
! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #282828, #a89984, #a89984
